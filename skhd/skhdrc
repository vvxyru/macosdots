cmd - return : /Applications/Alacritty.app/Contents/MacOS/alacritty

cmd - f : if [ "$(yabai -m query --spaces --space | jq -r '.type')" = "bsp" ]; then \
              yabai -m space --layout float; \
          else \
              yabai -m space --layout bsp; \
          fi

# focus windows
cmd - j : ymsp focus-down-window
cmd - k : ymsp focus-up-window

# adjust number of master windows
cmd + shift - i : ymsp increase-master-window-count
cmd + shift - d : ymsp decrease-master-window-count
